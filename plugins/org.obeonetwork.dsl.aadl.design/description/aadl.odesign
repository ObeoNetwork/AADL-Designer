<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.obeo.fr/dsl/viewpoint/description/1.1.0" xmlns:style="http://www.obeo.fr/dsl/viewpoint/description/style/1.1.0" xmlns:tool="http://www.obeo.fr/dsl/viewpoint/description/tool/1.1.0" name="aadl">
  <ownedViewpoints name="AADL">
    <ownedRepresentations xsi:type="description:DiagramDescription" name="Package Diagram" domainClass="aadl2.AadlPackage">
      <defaultLayer name="Default">
        <nodeMappings name="Data" domainClass="aadl2.Data">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/data.svg"/>
        </nodeMappings>
        <nodeMappings name="Process" label="Process" domainClass="aadl2.Process">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/process.svg"/>
        </nodeMappings>
        <containerMappings name="SystemTypeContainer" domainClass="aadl2.SystemType" listContainer="true">
          <subNodeMappings name="SystemImplementation" domainClass="aadl2.SystemImplementation">
            <style xsi:type="style:BundledImageDescription" showIcon="false" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </style>
          </subNodeMappings>
          <style xsi:type="style:WorkspaceImageDescription" labelSize="10" labelFormat="bold" workspacePath="/org.obeonetwork.dsl.aadl.design/images/system.svg"/>
        </containerMappings>
        <toolSections name="Components">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="System" nodeMappings="//@ownedViewpoints[name='AADL']/@ownedRepresentations[name='Package%20Diagram']/@defaultLayer/@containerMappings[name='SystemTypeContainer']/@subNodeMappings[name='SystemImplementation']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[container/]">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="aadl2.NamedElement" referenceName="publicSection">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="['New_NamedElement'/]"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Comp_Data" nodeMappings="//@ownedViewpoints[name='AADL']/@ownedRepresentations[name='Package%20Diagram']/@defaultLayer/@nodeMappings[name='Data']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[container/]">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="aadl2.Data" referenceName="publicSection"/>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
  </ownedViewpoints>
  <ownedViewpoints name="AADL2" modelFileExtension="aadl">
    <ownedRepresentations xsi:type="description:DiagramDescription" name="Package Diagram" domainClass="aadl2.AadlPackage">
      <defaultLayer name="Default">
        <containerMappings name="PublicPackageSection" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.PublicPackageSection">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/package.svg"/>
        </containerMappings>
      </defaultLayer>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description:DiagramDescription" name="Declaration Diagram" domainClass="aadl2.PackageSection">
      <defaultLayer name="Default">
        <nodeMappings name="DataType" semanticCandidatesExpression="&lt;%eContents()%>" domainClass="aadl2.DataType">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/data.svg"/>
        </nodeMappings>
        <nodeMappings name="ThreadType" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.ThreadType">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/thread.svg"/>
        </nodeMappings>
        <nodeMappings name="ProcessType" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.ProcessType">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/process.svg"/>
        </nodeMappings>
        <nodeMappings name="BusType" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.BusType">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/bus.svg"/>
        </nodeMappings>
        <nodeMappings name="ProcessorType" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.ProcessorType">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/processor.svg"/>
        </nodeMappings>
        <nodeMappings name="ThreadGroupType" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.ThreadGroupType">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/threadgroup.svg"/>
        </nodeMappings>
        <nodeMappings name="SubprogramType" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.SubprogramType">
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/subprogram.svg"/>
        </nodeMappings>
        <containerMappings name="SystemType" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.SystemType">
          <borderedNodeMappings name="DataPort" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.DataPort">
            <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/data_inport.svg"/>
          </borderedNodeMappings>
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" workspacePath="/org.obeonetwork.dsl.aadl.design/images/system.svg"/>
        </containerMappings>
        <toolSections name="Software Components">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="DataType_CR" label="Datatype" nodeMappings="//@ownedViewpoints[name='AADL2']/@ownedRepresentations[name='Declaration%20Diagram']/@defaultLayer/@nodeMappings[name='DataType']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[target/]">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="aadl2.DataType" referenceName="ownedClassifier">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="New Data"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="ProcessType_CR" label="Processtype" nodeMappings="//@ownedViewpoints[name='AADL2']/@ownedRepresentations[name='Declaration%20Diagram']/@defaultLayer/@nodeMappings[name='ProcessType']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[target/]">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="aadl2.ProcessType" referenceName="ownedClassifier">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="New Process"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="ThreadType_CR" label="Threadtype" nodeMappings="//@ownedViewpoints[name='AADL2']/@ownedRepresentations[name='Declaration%20Diagram']/@defaultLayer/@nodeMappings[name='ThreadType']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[target/]">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="aadl2.ThreadType" referenceName="ownedClassifier">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="New Thread"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="ThreadGroupType_CR" label="Thread Group Type" nodeMappings="//@ownedViewpoints[name='AADL2']/@ownedRepresentations[name='Declaration%20Diagram']/@defaultLayer/@nodeMappings[name='ThreadGroupType']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[target/]">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="aadl2.ThreadGroupType" referenceName="ownedClassifier">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="New Thread Group"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="SubprogramType_CR" label="Subprogramtype" nodeMappings="//@ownedViewpoints[name='AADL2']/@ownedRepresentations[name='Declaration%20Diagram']/@defaultLayer/@nodeMappings[name='SubprogramType']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="[target/]">
                <subModelOperations xsi:type="tool:CreateInstance" typeName="aadl2.SubprogramType" referenceName="ownedClassifier">
                  <subModelOperations xsi:type="tool:SetValue" featureName="name" valueExpression="New Subprogram"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
        <toolSections name="Execution Platform Components"/>
      </defaultLayer>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description:DiagramDescription" name="Implementation Diagram" domainClass="aadl2.PackageSection">
      <defaultLayer name="Default">
        <containerMappings name="SystemImplementation" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.SystemImplementation">
          <subNodeMappings name="SubSystem" semanticCandidatesExpression="[eContents()/]" domainClass="aadl2.SystemSubcomponent">
            <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/system.svg"/>
          </subNodeMappings>
          <style xsi:type="style:WorkspaceImageDescription" showIcon="false" sizeComputationExpression="-1" resizeKind="NSEW" workspacePath="/org.obeonetwork.dsl.aadl.design/images/system.svg"/>
        </containerMappings>
      </defaultLayer>
    </ownedRepresentations>
  </ownedViewpoints>
</description:Group>
